CREATE TABLE INST(
    INSTITUTE_CODE INT PRIMARY KEY,
    INSTITUTE_NAME VARCHAR(100),
    ESTABLISHMENT_YEAR INT,
    NIRF INT,
    IMAGE_SRC VARCHAR(100),
    STATE VARCHAR(50),
    INSTITUTE_ADDRESS VARCHAR(100),
    CTC FLOAT
);
